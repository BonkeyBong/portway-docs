    "gatsby-source-filesystem": "^2.1.56",
    "marked": "^0.8.1",
    "mobx": "^5.15.4",
    "node-sass": "^4.13.1",
    "openapi-filter": "^1.5.1",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "redoc": "^2.0.0-rc.23",
    "speccy": "^0.11.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react-hooks": "^2.3.0",
