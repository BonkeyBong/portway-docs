{
  "name": "portway-docs",
  "version": "1.0.0",
  "description": "API docs for Portway",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node index.js",
    "generate-docs": "npx openapi-filter --strip -- docs/openapi.yml docs/pre-processed.yml && npx redoc-cli bundle docs/pre-processed.yml -o output/api.html --options.hideDownloadButton=true",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BonkeyBong/portway-docs.git"
  },
  "author": "BonkeyBong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BonkeyBong/portway-docs/issues"
  },
  "homepage": "https://github.com/BonkeyBong/portway-docs#readme",
  "devDependencies": {
    "openapi-filter": "^1.5.1",
    "redoc": "^2.0.0-rc.20",
    "speccy": "^0.11.0"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
