    "eslint": "^6.8.0",
    "prettier": "^1.19.1"
  },
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "build": "npm run generate-spec && gatsby build",
    "develop": "npm run generate-spec && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "generate-spec": "cp -r spec/definitions static/spec/definitions && npx openapi-filter --strip -- spec/openapi.yml static/spec/openapi.yml"
  },
